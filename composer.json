{
    "name": "edspc/oauth-http-client-bundle",
    "description": "OAuth HTTP Client based on Symfony HTTP Client",
    "type": "symfony-bundle",
    "version": "0.1.0",
    "require": {
        "php": "^7.4",
        "psr/cache": "*",
        "symfony/http-client": "5.4.*"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Edspc\\OauthHttpClient\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Eduard Muradov",
            "homepage": "https://github.com/edspc"
        }
    ],
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    }
}
